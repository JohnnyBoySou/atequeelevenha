{
  "expo": {
    "name": "Até que Ele venha",
    "slug": "atequeelevenha",
    "version": "1.7.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "schema": "com.joaosousa.atequeelevenha",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF7EC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF7EC"
      },
      "package": "com.joaosousa.atequeelevenha"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "extra": {
      "eas": {
        "projectId": "0795cae4-8ee0-4b9a-ac22-91fe3f88c749"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-av",
        {
          "microphonePermission": "Permita o Até que ele venha a acessar o microfone."
        }
      ]
    ]
  }
}
